{
  "name": "express-route-config",
  "version": "0.0.1",
  "description": "",
  "keywords": [],
  "main": "dist/express-route-config.js",
  "module": "dist/express-route-config.js",
  "typings": "dist/types/express-route-config.d.ts",
  "files": ["dist"],
  "author": "Åukasz Sentkiewicz",
  "repository": {
    "type": "git",
    "url": "https://github.com/effectivex/express-route-config"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build":
      "tsc -p . --declaration && typedoc --out docs --target es6 --theme minimal --mode file src",
    "jest": "jest",
    "test": "npm run lint && npm run jest -- --coverage --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "coveragePathIgnorePatterns": ["/node_modules/", "/test/"],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/http-errors": "^1.6.1",
    "@types/jest": "^22.0.0",
    "@types/node": "^9.3.0",
    "@types/supertest": "^2.0.4",
    "cross-env": "^5.0.1",
    "express": "^4.16.3",
    "husky": "^0.14.0",
    "jest": "^22.0.2",
    "lint-staged": "^7.0.0",
    "prettier": "^1.4.4",
    "rimraf": "^2.6.1",
    "supertest": "^3.0.0",
    "ts-jest": "^22.0.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.1.0",
    "tslint-config-standard": "^7.0.0",
    "typedoc": "^0.11.0",
    "typescript": "^2.6.2",
    "validate-commit-msg": "^2.12.2"
  },
  "peerDependencies": {
    "express": "^4"
  },
  "dependencies": {
    "http-errors": "^1.6.2"
  }
}
